-- Returns the absolute value of -1 (which is 1)
SELECT abs(-1);

-- Returns the cosine of 90 degrees (value in radians)
SELECT cos(90);

-- Returns the smallest integer greater than or equal to 3.1 (which is 4)
SELECT CEILING(3.1);

-- Returns the greatest value from the list (which is 7)
SELECT GREATEST(1,2,3,4,5,6,7);

-- Performs integer division of 10 by 5 (result is 2)
SELECT 10 DIV 5;

-- Returns the natural logarithm of 2
SELECT LOG(2);

-- Returns the base-10 logarithm of 100 (which is 2)
SELECT LOG10(100);

-- Truncates the number 100.5 to 3 decimal places (returns 100.500)
SELECT TRUNCATE(100.5,3);

-- Returns 2 raised to the power of 3 (which is 8)
SELECT POW(2,3);

-- Returns the square root of 16 (which is 4)
SELECT SQRT(16);





